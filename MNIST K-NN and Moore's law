    "#Train a linear model to fit your plot\n",
    "\n",
    "import numpy as np\n",
    "from sklearn import datasets, linear_model\n",
    "from sklearn.metrics import mean_squared_error, r2_score\n",
    "from sklearn.model_selection  import train_test_split as split\n",
    "from sklearn.metrics import mean_absolute_error\n",
    "from sklearn.metrics import median_absolute_error\n",
    "\n",
    "#getting the log value for base\n",
    "data['log_base'] =  np.log(data.base)\n",
    "\n",
    "#defining the x variable as dates for the training data\n",
    "X=data['dates']\n",
    "#converting timestamp to fit the required types by LinearRegression()\n",
    "#converting the array into list for LinearRegression()\n",
    "X= list(map(lambda x: [int(round(x.timestamp()))], X))\n",
    "#defining the y variable as log_base for the training data\n",
    "Y=data['log_base']\n",
    "#converting the array into list for LinearRegression()\n",
    "Y= list(Y)\n",
    "\n",
    "#splitting the data for training as test and train\n",
    "X_train, X_test, Y_train, Y_test = split(X, Y, test_size=0.25)\n",
    "\n",
    "#linear regression model\n",
    "lm = linear_model.LinearRegression()\n",
    "lm.fit(X_train, Y_train)\n",
    "#prediction of y according X_test by using lm\n",
    "Y_predict = lm.predict(X_test)\n",
    "\n",
    "print(\"Mean squared error (MSE):\" , mean_squared_error(Y_test, Y_predict))\n",
    "print('The coefficient of determination (or R²):' , r2_score(Y_test, Y_predict))\n",
    "print(\"Mean absulate error (MAE):\" , mean_absolute_error(Y_test, Y_predict))\n",
    "print(\"Median absulate error:\" , median_absolute_error(Y_test, Y_predict))\n",
    "\n",
    "\n"
   ]

   "source": [
    "import datetime as dt\n",
    "\n",
    "#Date is the middle part of the test id. We need to obtain the date from the string\n",
    "#example: 981007 will convert as 07/10/1998\n",
    "#I will define converter to convert test_id into dates\n",
    "\n",
    "def converter(test_id):\n",
    "    #removes the \"cpu95-\"\n",
    "    date=test_id[6:]\n",
    "    #removes the part after \"-\"\n",
    "    date=date[:date.find('-')]\n",
    "    #if year is 4 digit, gets rid of first two digit to uniform the data\n",
    "    if len(date) > 6:\n",
    "        date = date[2:]\n",
    "    #returns the string as a time stamp\n",
    "    return dt.datetime.strptime(date, \"%y%m%d\")\n",
    "\n",
    "#empty list to store the dates\n",
    "dates = []\n",
    "\n",
    "#for loop to iterate the converter for all the row in the 'testid' columns\n",
    "for row in data['testID']:\n",
    "    dates.append(converter(row))\n",
    "\n",
    "data['dates']=dates    \n"
   ]

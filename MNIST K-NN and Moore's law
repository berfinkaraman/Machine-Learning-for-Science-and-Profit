    "from sklearn import neighbors, metrics\n",
    "from sklearn.metrics import accuracy_score\n",
    "from sklearn.model_selection import cross_val_score\n",
    "\n",
    "#defining the k \n",
    "k_neighbors = 3\n",
    "\n",
    "#creating emty lists to store x and y\n",
    "x = []\n",
    "y = []\n",
    "\n",
    "\n",
    "length = len(digits.target)\n",
    "\n",
    "#for loop to store data and targets for the digits 1 and 7\n",
    "for i in range(length): \n",
    "    if any(digits.target) == 1:\n",
    "        x.append(digits.data[i])\n",
    "        y.append(digits.target[i])\n",
    "    elif any(digits.target) == 7:\n",
    "        x.append(digits.data[i])\n",
    "        y.append(digits.target[i])\n",
    "\n",
    "#splitting the data for training\n",
    "x_train, x_test, y_train, y_test = split(x, y, test_size=0.5)\n",
    "\n",
    "#defining and fitting the knn model \n",
    "knn=neighbors.KNeighborsClassifier(k_neighbors, weights='uniform')\n",
    "knn.fit(x_train, y_train)\n",
    "#prediction of y by using x_test\n",
    "pred = knn.predict(x_test)\n",
    "\n",
    "#printing the evaluation to find the error rate\n",
    "print(\"Classification report for kNN %s:\\n%s\\n\"% (knn, metrics.classification_report(y_test, pred)))\n",
    "print(\"Confusion matrix:\\n%s\" % metrics.confusion_matrix(y_test, pred))\n",
    "\n",
    "\n",
